<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:topRightRadius="@dimen/gts_side_corner_radius" android:bottomRightRadius="@dimen/gts_side_corner_radius" />
            <solid android:color="@color/gst_tab_btn_bg" />
            <stroke android:width="@dimen/gts_divider_width" android:color="@color/gst_tab_btn_border" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:topRightRadius="@dimen/gts_side_corner_radius" android:bottomRightRadius="@dimen/gts_side_corner_radius" />
            <solid android:color="@color/high_light_orange" />
        </shape>
    </item>
</selector>
