<?xml version="1.0" encoding="utf-8"?>
<miuix.springback.view.SpringBackLayout android:layout_width="fill_parent" android:layout_height="fill_parent" app:scrollableView="@id/scrollView"
  xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.core.widget.NestedScrollView android:id="@id/scrollView" android:scrollbars="vertical" android:fadingEdge="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent" android:fillViewport="true">
        <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="fill_parent">
            <LinearLayout android:orientation="vertical" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/card_margin_top" android:layout_marginBottom="@dimen/card_margin_bottom">
                <com.miui.calculator.tax.TaxCardView android:layout_gravity="center_horizontal" android:id="@id/tax_card_view" android:layout_width="fill_parent" android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout android:orientation="vertical" android:id="@id/lyt_detailed_List_middle" android:layout_width="fill_parent" android:layout_height="wrap_content">
                <TextView android:text="@string/tax_insurance_and_fund_detail" android:layout_marginVertical="@dimen/result_title_margin_vertical" style="@style/tax_result_title_style" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_accumulation_fund" android:layout_width="fill_parent" android:layout_height="wrap_content" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_medical" android:layout_width="fill_parent" android:layout_height="wrap_content" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_endowment" android:layout_width="fill_parent" android:layout_height="wrap_content" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_unemployment" android:layout_width="fill_parent" android:layout_height="wrap_content" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_injuery" android:layout_width="fill_parent" android:layout_height="wrap_content" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_birth" android:layout_width="fill_parent" android:layout_height="wrap_content" />
            </LinearLayout>
            <View android:id="@id/divider_middle" android:background="@color/divider_item" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="@dimen/city_picker_item_divider" android:layout_marginVertical="@dimen/divider_middle_margin_vertical" />
            <LinearLayout android:orientation="vertical" android:id="@id/lyt_detailed_List_bottom" android:visibility="gone" android:layout_width="fill_parent" android:layout_height="wrap_content" android:layout_marginBottom="@dimen/detailed_List_bottom_margin_bottom">
                <TextView android:text="@string/tax_detail" android:layout_marginVertical="@dimen/result_title_margin_vertical" style="@style/tax_result_title_style" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_tax_amount" android:layout_width="fill_parent" android:layout_height="wrap_content" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_tax_rate" android:layout_width="fill_parent" android:layout_height="wrap_content" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_tax_take_off" android:layout_width="fill_parent" android:layout_height="wrap_content" />
                <com.miui.calculator.common.widget.ResultItemView android:id="@id/riv_tax_payed" android:layout_width="fill_parent" android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</miuix.springback.view.SpringBackLayout>
